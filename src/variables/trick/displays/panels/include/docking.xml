<?xml version="1.0"?>
<Dummy>
    <Container X="#DisplayX" Y="#DisplayY" Width="#DisplayWidth" Height="#DisplayHeight">
        <If Operator="eq" Value1="@MPCV_XFR_ATTACH_STATE" Value2="0">
            <True>
                <Image>${dcappDisplayHome}/modules/subsystems/images/docking.tga</Image>
            </True>
            <False>
                <Image>${dcappDisplayHome}/modules/subsystems/images/nondocking.tga</Image>
            </False>
        </If>
        <String Y="780" HorizontalAlign="Center" VerticalAlign="Top">#VisitingVechileName</String>
        <String Y="20" HorizontalAlign="Center" VerticalAlign="Bottom">#HabModuleName</String>

        <If Value="@ORION_TO_VESTIBULE_HATCH_OPEN_IND">
            <True>
                <If Value="@ORION_TO_VESTIBULE_HATCH_FLOW_POS">
                    <Line LineWidth="2" Color="#LineActive">
                        <Vertex X="550" Y="570"/>
                        <Vertex X="550" Y="450"/>
                    </Line>
                    <Polygon FillColor="#LineActive">
                        <Vertex X="550" Y="450"/>
                        <Vertex X="540" Y="470"/>
                        <Vertex X="560" Y="470"/>
                    </Polygon>
                    <Container Y="450" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Top">
                        <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
                        <String Size="#VerySmallFont" X="4" HorizontalAlign="Left" VerticalAlign="Middle" Color="#FgColor">Flow:</String>
                        <String Size="#VerySmallFont" X="196" HorizontalAlign="Right" VerticalAlign="Middle" Color="#FgColor">@ORION_TO_VESTIBULE_HATCH_FLOW_RATE_DISP(%.1f)lb/hr</String>
                    </Container>
                </If>
                <If Value="@ORION_TO_VESTIBULE_HATCH_FLOW_NEG">
                    <Line LineWidth="2" Color="#LineActive">
                        <Vertex X="550" Y="570"/>
                        <Vertex X="550" Y="450"/>
                    </Line>
                    <Polygon FillColor="#LineActive">
                        <Vertex X="550" Y="570"/>
                        <Vertex X="540" Y="550"/>
                        <Vertex X="560" Y="550"/>
                    </Polygon>
                    <Container Y="450" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Top">
                        <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
                        <String Size="#VerySmallFont" X="4" HorizontalAlign="Left" VerticalAlign="Middle" Color="#FgColor">Flow:</String>
                        <String Size="#VerySmallFont" X="196" HorizontalAlign="Right" VerticalAlign="Middle" Color="#FgColor">@ORION_TO_VESTIBULE_HATCH_FLOW_RATE_DISP(%.1f)lb/hr</String>
                    </Container>
                </If>
                <Image Y="520" Width="120" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="-90">${dcappDisplayHome}/modules/subsystems/images/hatch_open.tga</Image>
            </True>
            <False>
                <Image Y="520" Width="120" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="-90">${dcappDisplayHome}/modules/subsystems/images/hatch_closed.tga</Image>
            </False>
        </If>

        <If Operator="ge" Value1="@ORION_TO_VESTIBULE_MPEV_FLOW_DISP" Value2=".05">
            <True>
                <Line LineWidth="2" Color="#LineActive">
                    <Vertex X="630" Y="624"/>
                    <Vertex X="630" Y="470"/>
                </Line>
                <If Operator="ge" Value1="@ORION_TO_VESTIBULE_MPEV_FLOW_RATE" Value2="0">
                    <True>
                        <Polygon FillColor="#LineActive">
                            <Vertex X="640" Y="490"/>
                            <Vertex X="630" Y="470"/>
                            <Vertex X="620" Y="490"/>
                        </Polygon>
                    </True>
                    <False>
                        <Polygon FillColor="#LineActive">
                            <Vertex X="620" Y="604"/>
                            <Vertex X="630" Y="624"/>
                            <Vertex X="640" Y="604"/>
                        </Polygon>
                    </False>
                </If>
            </True>
            <False>
                <Line LineWidth="2" Color="#LineInactive">
                    <Vertex X="630" Y="624"/>
                    <Vertex X="630" Y="470"/>
                </Line>
            </False>
        </If>
        <If Value="@ORION_TO_VESTIBULE_MPEV_POS_IND">
            <True>
                <Image X="630" Y="550" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="-90">${dcappDisplayHome}/modules/subsystems/images/valve_on.tga</Image>
            </True>
            <False>
                <Image X="630" Y="550" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="-90">${dcappDisplayHome}/modules/subsystems/images/valve_off.tga</Image>
            </False>
        </If>
        <Container X="630" Y="656" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Bottom">
            <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
            <String Size="#VerySmallFont" HorizontalAlign="Center" VerticalAlign="Middle" Color="#FgColor">PE Valve</String>
        </Container>
        <Container X="630" Y="656" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Top">
            <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
            <String Size="#VerySmallFont" X="4" HorizontalAlign="Left" VerticalAlign="Middle" Color="#FgColor">Flow:</String>
            <String Size="#VerySmallFont" X="196" HorizontalAlign="Right" VerticalAlign="Middle" Color="#FgColor">@ORION_TO_VESTIBULE_MPEV_FLOW_DISP(%.1f)lb/hr</String>
        </Container>

        <If Value="@EAM_TO_VESTIBULE_HATCH_OPEN_IND">
            <True>
                <If Value="@EAM_TO_VESTIBULE_HATCH_FLOW_POS">
                    <Line LineWidth="2" Color="#LineActive">
                        <Vertex X="550" Y="350"/>
                        <Vertex X="550" Y="230"/>
                    </Line>
                    <Polygon FillColor="#LineActive">
                        <Vertex X="550" Y="350"/>
                        <Vertex X="540" Y="330"/>
                        <Vertex X="560" Y="330"/>
                    </Polygon>
                    <Container Y="350" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Bottom">
                        <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
                        <String Size="#VerySmallFont" X="4" HorizontalAlign="Left" VerticalAlign="Middle" Color="#FgColor">Flow:</String>
                        <String Size="#VerySmallFont" X="196" HorizontalAlign="Right" VerticalAlign="Middle" Color="#FgColor">@EAM_TO_VESTIBULE_HATCH_FLOW_RATE_DISP(%.1f)lb/hr</String>
                    </Container>
                </If>
                <If Value="@EAM_TO_VESTIBULE_HATCH_FLOW_NEG">
                    <Line LineWidth="2" Color="#LineActive">
                        <Vertex X="550" Y="350"/>
                        <Vertex X="550" Y="230"/>
                    </Line>
                    <Polygon FillColor="#LineActive">
                        <Vertex X="550" Y="230"/>
                        <Vertex X="540" Y="250"/>
                        <Vertex X="560" Y="250"/>
                    </Polygon>
                    <Container Y="350" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Bottom">
                        <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
                        <String Size="#VerySmallFont" X="4" HorizontalAlign="Left" VerticalAlign="Middle" Color="#FgColor">Flow:</String>
                        <String Size="#VerySmallFont" X="196" HorizontalAlign="Right" VerticalAlign="Middle" Color="#FgColor">@EAM_TO_VESTIBULE_HATCH_FLOW_RATE_DISP(%.1f)lb/hr</String>
                    </Container>
                </If>
                <Image Y="280" Width="120" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="90">${dcappDisplayHome}/modules/subsystems/images/hatch_open.tga</Image>
            </True>
            <False>
                <Image Y="280" Width="120" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="90">${dcappDisplayHome}/modules/subsystems/images/hatch_closed.tga</Image>
            </False>
        </If>

        <If Operator="ge" Value1="@EAM_TO_VESTIBULE_MPEV_FLOW_DISP" Value2=".05">
            <True>
                <Line LineWidth="2" Color="#LineActive">
                    <Vertex X="630" Y="176"/>
                    <Vertex X="630" Y="330"/>
                </Line>
                <If Operator="ge" Value1="@EAM_TO_VESTIBULE_MPEV_FLOW_RATE" Value2="0">
                    <True>
                        <Polygon FillColor="#LineActive">
                            <Vertex X="620" Y="310"/>
                            <Vertex X="630" Y="330"/>
                            <Vertex X="640" Y="310"/>
                        </Polygon>
                    </True>
                    <False>
                        <Polygon FillColor="#LineActive">
                            <Vertex X="640" Y="196"/>
                            <Vertex X="630" Y="176"/>
                            <Vertex X="620" Y="196"/>
                        </Polygon>
                    </False>
                </If>
            </True>
            <False>
                <Line LineWidth="2" Color="#LineInactive">
                    <Vertex X="630" Y="176"/>
                    <Vertex X="630" Y="330"/>
                </Line>
            </False>
        </If>
        <If Value="@EAM_TO_VESTIBULE_MPEV_POS_IND">
            <True>
                <Image X="630" Y="250" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="-90">${dcappDisplayHome}/modules/subsystems/images/valve_on.tga</Image>
            </True>
            <False>
                <Image X="630" Y="250" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="-90">${dcappDisplayHome}/modules/subsystems/images/valve_off.tga</Image>
            </False>
        </If>
        <Container X="630" Y="144" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Bottom">
            <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
            <String Size="#VerySmallFont" HorizontalAlign="Center" VerticalAlign="Middle" Color="#FgColor">PE Valve</String>
        </Container>
        <Container X="630" Y="144" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Top">
            <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
            <String Size="#VerySmallFont" X="4" HorizontalAlign="Left" VerticalAlign="Middle" Color="#FgColor">Flow:</String>
            <String Size="#VerySmallFont" X="196" HorizontalAlign="Right" VerticalAlign="Middle" Color="#FgColor">@EAM_TO_VESTIBULE_MPEV_FLOW_RATE(%.1f)lb/hr</String>
        </Container>

        <If Operator="eq" Value1="@MPCV_XFR_ATTACH_STATE" Value2="0">
            <True>
                <If Operator="ge" Value1="@ORION_TO_EAM_VESTIBULE_FLOW_DISP" Value2=".05">
                    <True>
                        <Line LineWidth="2" Color="#LineActive">
                            <Vertex X="470" Y="196"/>
                            <Vertex X="470" Y="310"/>
                            <Vertex X="418" Y="334"/>
                            <Vertex X="418" Y="466"/>
                            <Vertex X="470" Y="490"/>
                            <Vertex X="470" Y="604"/>
                        </Line>
                        <If Operator="ge" Value1="@ORION_TO_EAM_VESTIBULE_FLOW_RATE" Value2="0">
                            <True>
                                <Polygon FillColor="#LineActive">
                                    <Vertex X="408" Y="348"/>
                                    <Vertex X="418" Y="368"/>
                                    <Vertex X="428" Y="348"/>
                                </Polygon>
                            </True>
                            <False>
                                <Polygon FillColor="#LineActive">
                                    <Vertex X="428" Y="452"/>
                                    <Vertex X="418" Y="432"/>
                                    <Vertex X="408" Y="452"/>
                                </Polygon>
                            </False>
                        </If>
                        <If Operator="ge" Value1="@ORION_TO_EAM_VESTIBULE_IMV_FLOW_RATE" Value2="0">
                            <True>
                                <Polygon FillColor="#LineActive">
                                    <Vertex X="408" Y="348"/>
                                    <Vertex X="418" Y="368"/>
                                    <Vertex X="428" Y="348"/>
                                </Polygon>
                            </True>
                            <False>
                                <Polygon FillColor="#LineActive">
                                    <Vertex X="428" Y="452"/>
                                    <Vertex X="418" Y="432"/>
                                    <Vertex X="408" Y="452"/>
                                </Polygon>
                            </False>
                        </If>
                    </True>
                    <False>
                        <Line LineWidth="2" Color="#LineInactive">
                            <Vertex X="470" Y="196"/>
                            <Vertex X="470" Y="310"/>
                            <Vertex X="418" Y="334"/>
                            <Vertex X="418" Y="466"/>
                            <Vertex X="470" Y="490"/>
                            <Vertex X="470" Y="604"/>
                        </Line>
                    </False>
                </If>
                <Container X="340" Y="400" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Bottom">
                    <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
                    <String Size="#VerySmallFont" HorizontalAlign="Center" VerticalAlign="Middle" Color="#FgColor">IMV Duct</String>
                </Container>
                <Container X="340" Y="400" Width="200" Height="32" HorizontalAlign="Center" VerticalAlign="Top">
                    <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
                    <String Size="#VerySmallFont" X="4" HorizontalAlign="Left" VerticalAlign="Middle" Color="#FgColor">Flow:</String>
                    <String Size="#VerySmallFont" X="196" HorizontalAlign="Right" VerticalAlign="Middle" Color="#FgColor">@ORION_TO_EAM_VESTIBULE_FLOW_DISP(%.0f) cfm</String>
                </Container>
            </True>
            <False>
                <Line LineWidth="2" Color="#LineInactive">
                    <Vertex X="470" Y="604"/>
                    <Vertex X="470" Y="470"/>
                </Line>
                <Line LineWidth="2" Color="#LineInactive">
                    <Vertex X="470" Y="196"/>
                    <Vertex X="470" Y="330"/>
                </Line>
            </False>
        </If>

        <Container X="260" Y="604" Width="240" Height="120" VerticalAlign="Bottom">
            <Rectangle FillColor="0.6 0.6 0.6" LineColor="0.4 0.4 0.4" LineWidth="3"/>
            <String Size="#SmallFont" HorizontalAlign="Center" VerticalAlign="Middle" Color="0.2 0.2 0.2">#VisitingVechileName\nECLSS</String>
        </Container>
        <Container X="260" Y="196" Width="240" Height="120" VerticalAlign="Top">
            <Rectangle FillColor="0.6 0.6 0.6" LineColor="0.4 0.4 0.4" LineWidth="3"/>
            <String Size="#SmallFont" HorizontalAlign="Center" VerticalAlign="Middle" Color="0.2 0.2 0.2">#HabModuleName\nECLSS</String>
        </Container>
        <If Value="@ORION_TO_VESTIBULE_IMV_POS_IND">
            <True>
                <Image X="470" Y="550" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="90">${dcappDisplayHome}/modules/subsystems/images/valve_on.tga</Image>
            </True>
            <False>
                <Image X="470" Y="550" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="90">${dcappDisplayHome}/modules/subsystems/images/valve_off.tga</Image>
            </False>
        </If>
        <If Value="@EAM_TO_VESTIBULE_IMV_POS_IND">
            <True>
                <Image X="470" Y="250" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="90">${dcappDisplayHome}/modules/subsystems/images/valve_on.tga</Image>
            </True>
            <False>
                <Image X="470" Y="250" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="90">${dcappDisplayHome}/modules/subsystems/images/valve_off.tga</Image>
            </False>
        </If>

        <Constant Name="PARAM_SIM_PRE_NAME">ORION</Constant>
        <Container X="880" Y="660" Width="240" Height="250" HorizontalAlign="Center" VerticalAlign="Middle">
            <Include>${dcappDisplayHome}/modules/subsystems/panels/cabin_info.xml</Include>
        </Container>

        <If Operator="eq" Value1="@MPCV_XFR_ATTACH_STATE" Value2="0">
            <Container X="800" Y="400" Width="240" Height="64" HorizontalAlign="Center" VerticalAlign="Middle">
                <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
                <String Size="#VerySmallFont" X="4" Y="62" HorizontalAlign="Left" VerticalAlign="Top" Color="#FgColor">Temp:</String>
                <String Size="#VerySmallFont" X="236" Y="62" HorizontalAlign="Right" VerticalAlign="Top" Color="#FgColor">@VESTIBULE_TEMP(%.1f)°F</String>
                <String Size="#VerySmallFont" X="4" Y="31" HorizontalAlign="Left" VerticalAlign="Top" Color="#FgColor">Pressure:</String>
                <String Size="#VerySmallFont" X="236" Y="31" HorizontalAlign="Right" VerticalAlign="Top" Color="#FgColor">@VESTIBULE_PRESS(%.2f)psi</String>
            </Container>
        </If>

        <Constant Name="PARAM_SIM_PRE_NAME">EAM</Constant>
        <Container X="880" Y="140" Width="240" Height="250" HorizontalAlign="Center" VerticalAlign="Middle">
            <Include>${dcappDisplayHome}/modules/subsystems/panels/cabin_info.xml</Include>
        </Container>
    </Container>

    <!-- Bottom Bezel Gutter -->
    <Container X="#b1x" Y="#bguttery" Width="#ButtonWidth3" Height="#GutterWidth">
        <Line LineWidth="1" Color="#FgColor">
            <Vertex X="#bgutterlinex1" Y="#bgutterliney1"/>
            <Vertex X="#bgutterlinex2" Y="#bgutterliney2"/>
            <Vertex X="#bgutterlinex3" Y="#bgutterliney3"/>
            <Vertex X="#bgutterlinex4" Y="#bgutterliney4"/>
        </Line>
        <String Style="BottomGutter">#VisitingVechileName</String>
    </Container>
    <Container X="#b4x" Y="#bguttery" Width="#ButtonWidth3" Height="#GutterWidth">
        <Line LineWidth="1" Color="#FgColor">
            <Vertex X="#bgutterlinex1" Y="#bgutterliney1"/>
            <Vertex X="#bgutterlinex2" Y="#bgutterliney2"/>
            <Vertex X="#bgutterlinex3" Y="#bgutterliney3"/>
            <Vertex X="#bgutterlinex4" Y="#bgutterliney4"/>
        </Line>
        <String Style="BottomGutter">#HabModuleName</String>
    </Container>

    <!-- Bottom Bezel -->
    <Button Style="BottomBezelButton" Type="Toggle" X="#b1x" Y="#by" BezelKey="#b1" SwitchVariable="@ORION_TO_VESTIBULE_IMV_POS_CMD" IndicatorVariable="@ORION_TO_VESTIBULE_IMV_POS_IND">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="BottomBezelButton">#VisitingVechileName\nIMV</String>
    </Button>
    <Button Style="BottomBezelButton" Type="Toggle" X="#b2x" Y="#by" BezelKey="#b2" SwitchVariable="@ORION_TO_VESTIBULE_HATCH_OPEN_CMD" IndicatorVariable="@ORION_TO_VESTIBULE_HATCH_OPEN_IND">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="BottomBezelButton">Hatch</String>
    </Button>
    <Button Style="BottomBezelButton" Type="Toggle" X="#b3x" Y="#by" BezelKey="#b3" SwitchVariable="@ORION_TO_VESTIBULE_MPEV_POS_CMD" IndicatorVariable="@ORION_TO_VESTIBULE_MPEV_POS_IND">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="BottomBezelButton">PE Valve</String>
    </Button>
    <Button Style="BottomBezelButton" Type="Toggle" X="#b4x" Y="#by" BezelKey="#b4" SwitchVariable="@EAM_TO_VESTIBULE_IMV_POS_CMD" IndicatorVariable="@EAM_TO_VESTIBULE_IMV_POS_IND">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="BottomBezelButton">#HabModuleName Ovhd\nAft IMV</String>
    </Button>
    <Button Style="BottomBezelButton" Type="Toggle" X="#b5x" Y="#by" BezelKey="#b5" SwitchVariable="@EAM_TO_VESTIBULE_HATCH_OPEN_CMD" IndicatorVariable="@EAM_TO_VESTIBULE_HATCH_OPEN_IND">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="BottomBezelButton">Hatch</String>
    </Button>
    <Button Style="BottomBezelButton" Type="Toggle" X="#b6x" Y="#by" BezelKey="#b6" SwitchVariable="@EAM_TO_VESTIBULE_MPEV_POS_CMD" IndicatorVariable="@EAM_TO_VESTIBULE_MPEV_POS_IND">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="BottomBezelButton">PE Valve</String>
    </Button>

</Dummy>
