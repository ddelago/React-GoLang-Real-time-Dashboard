<?xml version="1.0"?>
<Dummy>
    <If Operator="gt" Value1="@SAL_TRAY_EXTENSION" Value2="0">
        <Container X="800" Y="530" Width="160" Height="220" HorizontalAlign="Center" VirtualHeight="1">
            <Rectangle Y="@SAL_TRAY_EXTENSION" Width="100" Height="0.5" HorizontalAlign="Center" VerticalAlign="Bottom" FillColor="0.4 0.4 0.4" LineColor="1 1 1" LineWidth="1"/>
        </Container>
    </If>
    <Set Variable="@SAL_DOOR_ANGLE_DISP">0</Set>
    <Set Variable="@SAL_DOOR_ANGLE_DISP" Operator="-=">@SAL_DOOR_ANGLE</Set>
    <Image X="800" Y="626" Width="180" Height="16" HorizontalAlign="Center" VerticalAlign="Bottom">${dcappDisplayHome}/modules/nexsys_common/images/door.tga</Image>
    <Image X="800" Y="480" Width="200" Height="150" HorizontalAlign="Center" VerticalAlign="Bottom">${dcappDisplayHome}/modules/nexsys_common/images/base.tga</Image>
    <Image X="890" Y="640" Width="180" Height="16" HorizontalAlign="Right" VerticalAlign="Bottom" Rotate="@SAL_DOOR_ANGLE_DISP">${dcappDisplayHome}/modules/nexsys_common/images/door.tga</Image>

    <!-- Bottom Bezel -->
    <Set Variable="@SAL_DOOR_OPENABLE">0</Set>
    <Set Variable="@SAL_DOOR_CLOSEABLE">0</Set>
    <Set Variable="@SAL_TRAY_EXTENDABLE">0</Set>
    <Set Variable="@SAL_TRAY_RETRACTABLE">0</Set>
    <If Operator="eq" Value1="@SAL_DOOR_STATE" Value2="2">
        <If Operator="eq" Value1="@SAL_TRAY_STATE" Value2="1">
            <Set Variable="@SAL_TRAY_EXTENDABLE">1</Set>
        </If>
        <If Operator="eq" Value1="@SAL_TRAY_STATE" Value2="2">
            <Set Variable="@SAL_TRAY_RETRACTABLE">1</Set>
        </If>
    </If>
    <If Operator="eq" Value1="@SAL_TRAY_STATE" Value2="1">
        <If Operator="eq" Value1="@SAL_DOOR_STATE" Value2="1">
            <Set Variable="@SAL_DOOR_OPENABLE">1</Set>
        </If>
        <If Operator="eq" Value1="@SAL_DOOR_STATE" Value2="2">
            <Set Variable="@SAL_DOOR_CLOSEABLE">1</Set>
        </If>
    </If>
    <Button Style="BottomBezelButton" X="#b2x" Y="#by" SwitchVariable="@SAL_DOOR_CMD" IndicatorVariable="@SAL_DOOR_STATE" On="1" ActiveVariable="@SAL_DOOR_CLOSEABLE" BezelKey="#b2">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <Active><String Style="BottomBezelButton">Close\nSAL\nDoor</String></Active>
        <Inactive><String Style="BottomBezelButton" Color="#Inactive">Close\nSAL\nDoor</String></Inactive>
    </Button>
    <Button Style="BottomBezelButton" X="#b3x" Y="#by" SwitchVariable="@SAL_DOOR_CMD" IndicatorVariable="@SAL_DOOR_STATE" On="2" ActiveVariable="@SAL_DOOR_OPENABLE" BezelKey="#b3">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <Active><String Style="BottomBezelButton">Open\nSAL\nDoor</String></Active>
        <Inactive><String Style="BottomBezelButton" Color="#Inactive">Open\nSAL\nDoor</String></Inactive>
    </Button>
    <Button Style="BottomBezelButton" X="#b4x" Y="#by" SwitchVariable="@SAL_TRAY_CMD" IndicatorVariable="@SAL_TRAY_STATE" On="1" ActiveVariable="@SAL_TRAY_RETRACTABLE" BezelKey="#b4">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <Active><String Style="BottomBezelButton">Retract\nSAL\nTray</String></Active>
        <Inactive><String Style="BottomBezelButton" Color="#Inactive">Retract\nSAL\nTray</String></Inactive>
    </Button>
    <Button Style="BottomBezelButton" X="#b5x" Y="#by" SwitchVariable="@SAL_TRAY_CMD" IndicatorVariable="@SAL_TRAY_STATE" On="2" ActiveVariable="@SAL_TRAY_EXTENDABLE" BezelKey="#b5">
        <On><Rectangle Style="BottomBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="BottomBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="BottomBezelButton" FillColor="#ButtonOff"/></Off>
        <Active><String Style="BottomBezelButton">Extend\nSAL\nTray</String></Active>
        <Inactive><String Style="BottomBezelButton" Color="#Inactive">Extend\nSAL\nTray</String></Inactive>
    </Button>

    <!-- Right Bezel -->
    <Set Variable="@VEH_RELEASE_ENABLED">0</Set>
    <If Operator="ne" Value1="@PAYLOAD_0_LOCATION_IND" Value2="-1"><Set Variable="@VEH_RELEASE_ENABLED">1</Set></If>
    <If Operator="ne" Value1="@PAYLOAD_1_LOCATION_IND" Value2="-1"><Set Variable="@VEH_RELEASE_ENABLED">1</Set></If>
    <If Operator="ne" Value1="@PAYLOAD_2_LOCATION_IND" Value2="-1"><Set Variable="@VEH_RELEASE_ENABLED">1</Set></If>
    <If Operator="ne" Value1="@PAYLOAD_3_LOCATION_IND" Value2="-1"><Set Variable="@VEH_RELEASE_ENABLED">1</Set></If>

    <Button Style="RightBezelButton" Type="Momentary" X="#rx" Y="#r4y" BezelKey="#r4" ActiveVariable="@VEH_RELEASE_ENABLED">
        <On><Rectangle Style="RightBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="RightBezelButton" FillColor="#ButtonOff"/></Off>
        <Active><String Style="RightBezelButton">Vehicle\nPayload\nRelease</String></Active>
        <Inactive><String Style="RightBezelButton" Color="#Inactive">Vehicle\nPayload\nRelease</String></Inactive>
        <OnPress><Set Variable="@VEH_PAYLOAD_RELEASE">1</Set></OnPress>
    </Button>

    <If Operator="eq" Value1="@PAYLOAD_ID" Value2="0">
        <Button Style="RightBezelButton" SwitchVariable="@PAYLOAD_0_LOCATION_CMD" IndicatorVariable="@PAYLOAD_0_LOCATION_IND" On="1" ActiveVariable="@PAYLOAD_0_CAPTURE_STATUS" ActiveOn="1" X="#rx" Y="#r5y" BezelKey="#r5">
            <On><Rectangle Style="RightBezelButton" FillColor="#ButtonOn"/></On>
            <Transition><Rectangle Style="RightBezelButton" FillColor="#ButtonTrans"/></Transition>
            <Off><Rectangle Style="RightBezelButton" FillColor="#ButtonOff"/></Off>
            <Active><String Style="RightBezelButton">Dock\nPayload\nto SAL</String></Active>
            <Inactive><String Style="RightBezelButton" Color="#Inactive">Dock\nPayload\nto SAL</String></Inactive>
            <OnPress><Set Variable="@PAYLOAD_CONTROL_UPDATE">1</Set></OnPress>
        </Button>
        <Button Style="RightBezelButton" SwitchVariable="@PAYLOAD_0_LOCATION_CMD" IndicatorVariable="@PAYLOAD_0_LOCATION_IND" On="0" ActiveVariable="@PAYLOAD_0_CAPTURE_STATUS" ActiveOn="1" X="#rx" Y="#r6y" BezelKey="#r6">
            <On><Rectangle Style="RightBezelButton" FillColor="#ButtonOn"/></On>
            <Transition><Rectangle Style="RightBezelButton" FillColor="#ButtonTrans"/></Transition>
            <Off><Rectangle Style="RightBezelButton" FillColor="#ButtonOff"/></Off>
            <Active><String Style="RightBezelButton">Dock\nPayload\nto SRV</String></Active>
            <Inactive><String Style="RightBezelButton" Color="#Inactive">Dock\nPayload\nto SRV</String></Inactive>
            <OnPress><Set Variable="@PAYLOAD_CONTROL_UPDATE">1</Set></OnPress>
        </Button>
    </If>
    <If Operator="eq" Value1="@PAYLOAD_ID" Value2="1">
        <Button Style="RightBezelButton" SwitchVariable="@PAYLOAD_1_LOCATION_CMD" IndicatorVariable="@PAYLOAD_1_LOCATION_IND" On="1" ActiveVariable="@PAYLOAD_1_CAPTURE_STATUS" ActiveOn="1" X="#rx" Y="#r5y" BezelKey="#r5">
            <On><Rectangle Style="RightBezelButton" FillColor="#ButtonOn"/></On>
            <Transition><Rectangle Style="RightBezelButton" FillColor="#ButtonTrans"/></Transition>
            <Off><Rectangle Style="RightBezelButton" FillColor="#ButtonOff"/></Off>
            <Active><String Style="RightBezelButton">Dock\nPayload\nto SAL</String></Active>
            <Inactive><String Style="RightBezelButton" Color="#Inactive">Dock\nPayload\nto SAL</String></Inactive>
            <OnPress><Set Variable="@PAYLOAD_CONTROL_UPDATE">1</Set></OnPress>
        </Button>
        <Button Style="RightBezelButton" SwitchVariable="@PAYLOAD_1_LOCATION_CMD" IndicatorVariable="@PAYLOAD_1_LOCATION_IND" On="0" ActiveVariable="@PAYLOAD_1_CAPTURE_STATUS" ActiveOn="1" X="#rx" Y="#r6y" BezelKey="#r6">
            <On><Rectangle Style="RightBezelButton" FillColor="#ButtonOn"/></On>
            <Transition><Rectangle Style="RightBezelButton" FillColor="#ButtonTrans"/></Transition>
            <Off><Rectangle Style="RightBezelButton" FillColor="#ButtonOff"/></Off>
            <Active><String Style="RightBezelButton">Dock\nPayload\nto SRV</String></Active>
            <Inactive><String Style="RightBezelButton" Color="#Inactive">Dock\nPayload\nto SRV</String></Inactive>
            <OnPress><Set Variable="@PAYLOAD_CONTROL_UPDATE">1</Set></OnPress>
        </Button>
    </If>
    <If Operator="eq" Value1="@PAYLOAD_ID" Value2="2">
        <Button Style="RightBezelButton" SwitchVariable="@PAYLOAD_2_LOCATION_CMD" IndicatorVariable="@PAYLOAD_2_LOCATION_IND" On="1" ActiveVariable="@PAYLOAD_2_CAPTURE_STATUS" ActiveOn="1" X="#rx" Y="#r5y" BezelKey="#r5">
            <On><Rectangle Style="RightBezelButton" FillColor="#ButtonOn"/></On>
            <Transition><Rectangle Style="RightBezelButton" FillColor="#ButtonTrans"/></Transition>
            <Off><Rectangle Style="RightBezelButton" FillColor="#ButtonOff"/></Off>
            <Active><String Style="RightBezelButton">Dock\nPayload\nto SAL</String></Active>
            <Inactive><String Style="RightBezelButton" Color="#Inactive">Dock\nPayload\nto SAL</String></Inactive>
            <OnPress><Set Variable="@PAYLOAD_CONTROL_UPDATE">1</Set></OnPress>
        </Button>
        <Button Style="RightBezelButton" SwitchVariable="@PAYLOAD_2_LOCATION_CMD" IndicatorVariable="@PAYLOAD_2_LOCATION_IND" On="0" ActiveVariable="@PAYLOAD_2_CAPTURE_STATUS" ActiveOn="1" X="#rx" Y="#r6y" BezelKey="#r6">
            <On><Rectangle Style="RightBezelButton" FillColor="#ButtonOn"/></On>
            <Transition><Rectangle Style="RightBezelButton" FillColor="#ButtonTrans"/></Transition>
            <Off><Rectangle Style="RightBezelButton" FillColor="#ButtonOff"/></Off>
            <Active><String Style="RightBezelButton">Dock\nPayload\nto SRV</String></Active>
            <Inactive><String Style="RightBezelButton" Color="#Inactive">Dock\nPayload\nto SRV</String></Inactive>
            <OnPress><Set Variable="@PAYLOAD_CONTROL_UPDATE">1</Set></OnPress>
        </Button>
    </If>
    <If Operator="eq" Value1="@PAYLOAD_ID" Value2="3">
        <Button Style="RightBezelButton" SwitchVariable="@PAYLOAD_3_LOCATION_CMD" IndicatorVariable="@PAYLOAD_3_LOCATION_IND" On="1" ActiveVariable="@PAYLOAD_3_CAPTURE_STATUS" ActiveOn="1" X="#rx" Y="#r5y" BezelKey="#r5">
            <On><Rectangle Style="RightBezelButton" FillColor="#ButtonOn"/></On>
            <Transition><Rectangle Style="RightBezelButton" FillColor="#ButtonTrans"/></Transition>
            <Off><Rectangle Style="RightBezelButton" FillColor="#ButtonOff"/></Off>
            <Active><String Style="RightBezelButton">Dock\nPayload\nto SAL</String></Active>
            <Inactive><String Style="RightBezelButton" Color="#Inactive">Dock\nPayload\nto SAL</String></Inactive>
            <OnPress><Set Variable="@PAYLOAD_CONTROL_UPDATE">1</Set></OnPress>
        </Button>
        <Button Style="RightBezelButton" SwitchVariable="@PAYLOAD_3_LOCATION_CMD" IndicatorVariable="@PAYLOAD_3_LOCATION_IND" On="0" ActiveVariable="@PAYLOAD_3_CAPTURE_STATUS" ActiveOn="1" X="#rx" Y="#r6y" BezelKey="#r6">
            <On><Rectangle Style="RightBezelButton" FillColor="#ButtonOn"/></On>
            <Transition><Rectangle Style="RightBezelButton" FillColor="#ButtonTrans"/></Transition>
            <Off><Rectangle Style="RightBezelButton" FillColor="#ButtonOff"/></Off>
            <Active><String Style="RightBezelButton">Dock\nPayload\nto SRV</String></Active>
            <Inactive><String Style="RightBezelButton" Color="#Inactive">Dock\nPayload\nto SRV</String></Inactive>
            <OnPress><Set Variable="@PAYLOAD_CONTROL_UPDATE">1</Set></OnPress>
        </Button>
    </If>
</Dummy>
