<?xml version="1.0"?>
<Dummy>
    <Container Style="CornerBezelButton" X="#lx" Y="#l1y">
        <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff"/>
        <Image Width="46" Height="52" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/nexsys_common/images/home.tga</Image>
        <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff 0.6" />
    </Container>

    <If Value="#MpcvGncPageActive">
      <Button Style="TopBezelButton" X="#t1x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#MpcvGncDisplay" BezelKey="#t1">
          <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
          <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
          <String Style="TopBezelButton">MPCV\nGN&amp;C</String>
      </Button>
    </If>

    <If Value="#PmGncPageActive">
      <Button Style="TopBezelButton" X="#t2x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#PmGncDisplay" BezelKey="#t2">
          <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
          <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
          <String Style="TopBezelButton">PM\nGN&amp;C</String>
      </Button>
    </If>

    <If Value="#RoboticPageActive">
      <Button Style="TopBezelButton" X="#t3x" Y="#ty" BezelKey="#t3">
          <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
          <String Style="TopBezelButton">Robotics\n&amp; Doors</String>
          <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
          <OnPress>
              <Set Variable="@SUBMENU1_DISPLAY">@ACTIVE_DISPLAY</Set>
              <Set Variable="@ACTIVE_DISPLAY">@SUBMENU10_DISPLAY</Set>
          </OnPress>
      </Button>
    </If>

    <If Value="#SubsystemPageActive">
      <Button Style="TopBezelButton" X="#t4x" Y="#ty" BezelKey="#t4">
          <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
          <String Style="TopBezelButton">Subsystems</String>
          <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
          <OnPress>
              <Set Variable="@SUBMENU1_DISPLAY">@ACTIVE_DISPLAY</Set>
              <Set Variable="@ACTIVE_DISPLAY">@SUBMENU5_DISPLAY</Set>
          </OnPress>
      </Button>
    </If>

    <If Value="#CameraPageActive">
      <Button Style="TopBezelButton" X="#t5x" Y="#ty" BezelKey="#t5">
          <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
          <String Style="TopBezelButton">Cameras\n&amp; Lights</String>
          <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
          <OnPress>
              <Set Variable="@SUBMENU1_DISPLAY">@ACTIVE_DISPLAY</Set>
              <Set Variable="@ACTIVE_DISPLAY">@SUBMENU2_DISPLAY</Set>
          </OnPress>
      </Button>
    </If>

    <If Value="#RemoteOpsPageActive">
      <Button Style="TopBezelButton" X="#t6x" Y="#ty" BezelKey="#t6">
          <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
          <String Style="TopBezelButton">Remote\nOperations</String>
          <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
          <OnPress>
              <Set Variable="@SUBMENU1_DISPLAY">@ACTIVE_DISPLAY</Set>
              <Set Variable="@ACTIVE_DISPLAY">@REMOTE_OPERATION_DISPLAY</Set>
          </OnPress>
      </Button>
    </If>

    <Container Style="CornerBezelButton" X="#rx" Y="#r1y">
        <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff"/>
        <Image Width="38" Height="52" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/nexsys_common/images/up.tga</Image>
        <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff 0.6" />
    </Container>

    <Set Variable="@MENU_LABEL"></Set>
</Dummy>
