<?xml version="1.0"?>
<Dummy>

  <!-- Docking page that shows MPCV and HAB -->
  <Constant Name="PARAM_SIM_PRE_NAME">EAM</Constant>
  <Panel DisplayIndex="#StackDockingDisplay" BackgroundColor="#BgColor" VirtualWidth="#PanelWidth" VirtualHeight="#PanelHeight">
      <Include>include/docking.xml</Include>
      <Include>include/stackSubsTopMenu.xml</Include>
      <Include>${dcappDisplayHome}/modules/nexsys_common/panels/standard_elements.xml</Include>
  </Panel>

  <!-- Airlock page that shows HAL (or Airlock) and HAB -->
  <If Operator="eq" Value1="#AirlockModuleName" Value2="HAL">
    <True>
      <Constant Name="PARAM_SIM_PRE_NAME">HAL</Constant>
      <Panel DisplayIndex="#AirlockCabinDisplay" BackgroundColor="#BgColor" VirtualWidth="#PanelWidth" VirtualHeight="#PanelHeight">
          <Include>include/hal_airlock.xml</Include>
          <Include>include/stackSubsTopMenu.xml</Include>
          <Include>${dcappDisplayHome}/modules/nexsys_common/panels/standard_elements.xml</Include>
      </Panel>
    </True>
    <False>
      <Constant Name="PARAM_SIM_PRE_NAME">AIRLOCK</Constant>
      <Panel DisplayIndex="#AirlockCabinDisplay" BackgroundColor="#BgColor" VirtualWidth="#PanelWidth" VirtualHeight="#PanelHeight">
          <Include>${dcappDisplayHome}/modules/subsystems/panels/airlock.xml</Include>
          <Include>include/stackSubsTopMenu.xml</Include>
          <Include>${dcappDisplayHome}/modules/nexsys_common/panels/standard_elements.xml</Include>
      </Panel>
    </False>
  </If>

  <!-- MPCV Subsystems pages -->
  <Include>mpcv_subsystems.xml</Include>

  <!-- HAB Subsystem pages -->
  <Include>hab_subsystems.xml</Include>

  <!-- HAL Subsystem pages -->
  <If Operator="eq" Value1="#AirlockModuleName" Value2="HAL">
    <Constant Name="PARAM_SIM_PRE_NAME">HAL</Constant>
    <Include>hal_subsystems.xml</Include>
  </If>

  <!-- Crew model pages -->
  <Panel DisplayIndex="#CrewDisplay" BackgroundColor="#BgColor" VirtualWidth="#PanelWidth" VirtualHeight="#PanelHeight">
      <Include>${dcappDisplayHome}/modules/subsystems/panels/crew.xml</Include>
      <Include>include/stackSubsTopMenu.xml</Include>
      <Include>${dcappDisplayHome}/modules/nexsys_common/panels/standard_elements.xml</Include>
  </Panel>

</Dummy>
