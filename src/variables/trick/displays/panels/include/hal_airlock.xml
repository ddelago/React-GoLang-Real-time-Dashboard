<?xml version="1.0"?>
<Dummy>
    <Container X="#DisplayX" Y="#DisplayY" Width="#DisplayWidth" Height="#DisplayHeight">
    	<!-- Hab cabin shape-->
        <Image X="0" Y="0" Width="82" Height="640">${dcappDisplayHome}/modules/subsystems/images/cabin_partial.tga</Image>
        <String X="40" Y="280" HorizontalAlign="Center" VerticalAlign="Middle" Rotate="90">#HabModuleName</String>
        <!--Vestibule cabin shape-->
        <Image X="80" Width="198" Height="200" HorizontalAlign="Left" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/cabin.tga</Image>
        <!-- Hal cabin shape-->
        <Image Width="548" Height="480" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/cabin.tga</Image>
        <String Y="600" HorizontalAlign="Center" VerticalAlign="Middle">#AirlockModuleName</String>
        <!-- Suit lock boxes -->
        <Container X="825" Y="480" Width="80" Height="120" HorizontalAlign="Center" VerticalAlign="Middle">
            <Rectangle FillColor="0.3 0.3 0.3" LineColor="1.0 1.0 1.0" LineWidth="1"/>
            <String Size="#SmallFont" HorizontalAlign="Center" VerticalAlign="Middle" Color="1.0 1.0 1.0">Suit\nLock\n1</String>
        </Container>
        <Container X="825" Y="320" Width="80" Height="120" HorizontalAlign="Center" VerticalAlign="Middle">
            <Rectangle FillColor="0.3 0.3 0.3" LineColor="1.0 1.0 1.0" LineWidth="1"/>
            <String Size="#SmallFont" HorizontalAlign="Center" VerticalAlign="Middle" Color="1.0 1.0 1.0">Suit\nLock\n2</String>
        </Container>

        <Constant Name="PARAM_SIM_PRE_NAME_old">#{PARAM_SIM_PRE_NAME}</Constant>

        <Constant Name="PARAM_SIM_PRE_NAME">HAL</Constant>
        <Container X="550" Y="250" Width="240" Height="250" HorizontalAlign="Center" VerticalAlign="Middle">
            <Include>${dcappDisplayHome}/modules/subsystems/panels/cabin_info.xml</Include>
        </Container>
        <!-- display status of the Hab -->
        <Constant Name="PARAM_SIM_PRE_NAME">EAM</Constant>
        <Container X="150" Y="100" Width="240" Height="250" HorizontalAlign="Center" VerticalAlign="Middle">
            <Include>${dcappDisplayHome}/modules/subsystems/panels/cabin_info.xml</Include>
        </Container>

        <Constant Name="PARAM_SIM_PRE_NAME">#{PARAM_SIM_PRE_NAME_old}</Constant>

        <Line LineWidth="2" Color="#LineInactive">
        	<Vertex X="0" Y="460"/>
        	<Vertex X="100" Y="460"/>
        	<Vertex X="115" Y="490"/>
        	<Vertex X="245" Y="490"/>
            <Vertex X="260" Y="460"/>
            <Vertex X="500" Y="460"/>
        </Line>
        <If Operator="ge" Value1="@HAL_IMV_3_FLOW_RATE" Value2="0.5">
	        <True>
	        	<Line LineWidth="2" Color="#LineActive">
        			<Vertex X="0" Y="460"/>
        			<Vertex X="100" Y="460"/>
        			<Vertex X="115" Y="490"/>
        			<Vertex X="250" Y="490"/>
          		  	<Vertex X="265" Y="460"/>
           		 	<Vertex X="500" Y="460"/>
        		</Line>
                <Polygon FillColor="#LineActive">
                    <Vertex X="160" Y="490"/>
                    <Vertex X="180" Y="500"/>
                    <Vertex X="180" Y="480"/>
                </Polygon>
            </True>
         </If>
         <If Operator="le" Value1="@HAL_IMV_3_FLOW_RATE" Value2="-0.5">
            <True>
             	<Line LineWidth="2" Color="#LineActive">
        			<Vertex X="0" Y="460"/>
        			<Vertex X="100" Y="460"/>
        			<Vertex X="115" Y="490"/>
        			<Vertex X="250" Y="490"/>
          		  	<Vertex X="265" Y="460"/>
           		 	<Vertex X="500" Y="460"/>
        		</Line>
                <Polygon FillColor="#LineActive">
                    <Vertex X="180" Y="490"/>
                    <Vertex X="160" Y="500"/>
                    <Vertex X="160" Y="480"/>
                </Polygon>
        	</True>
        </If>
        <Container X="82" Y="600" Width="200" Height="96" HorizontalAlign="Left" VerticalAlign="Top">
            <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
            <String Size="#VerySmallFont" Y="93" X="4" HorizontalAlign="Left" VerticalAlign="Top" Color="#FgColor">Temp:</String>
            <String Size="#VerySmallFont" Y="93" X="196" HorizontalAlign="Right" VerticalAlign="Top" Color="#FgColor">@HAL_VESTIBULE_TEMP(%.1f) Â°F</String>
			      <String Size="#VerySmallFont" Y="62" X="4" HorizontalAlign="Left" VerticalAlign="Top" Color="#FgColor">Press:</String>
            <String Size="#VerySmallFont" Y="62" X="196" HorizontalAlign="Right" VerticalAlign="Top" Color="#FgColor">@HAL_VESTIBULE_PRESS(%.2f) psi</String>
			      <String Size="#VerySmallFont" Y="31" X="4" HorizontalAlign="Left" VerticalAlign="Top" Color="#FgColor">IMV Flow:</String>
            <String Size="#VerySmallFont" Y="31" X="196" HorizontalAlign="Right" VerticalAlign="Top" Color="#FgColor">@HAL_IMV_3_FLOW_RATE_DISP(%.0f) cfm</String>
        </Container>
        <If Value="@DECK_AFT_IMV_POS_IND">
            <True>
                <Image X="50" Y="460" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/valve_on.tga</Image>
            </True>
            <False>
                <Image X="50" Y="460" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/valve_off.tga</Image>
            </False>
        </If>
        <If Value="@HAL_TO_EAM_HATCH_OPEN_IND">
            <True>
                 <If Operator="gt" Value1="@HAL_TO_EAM_HATCH_FLOW_DISP" Value2="0.5">
                    <Line LineWidth="2" Color="#LineActive">
                        <Vertex X="30" Y="400"/>
                        <Vertex X="130" Y="400"/>
                    </Line>
                    <Container X="72" Y="350" Width="120" Height="32" HorizontalAlign="Center" VerticalAlign="Top">
                        <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
                        <String Size="#VerySmallFont" X="116" HorizontalAlign="Right" VerticalAlign="Middle" Color="#FgColor">@HAL_TO_EAM_HATCH_FLOW_DISP(%.0f)lb/hr</String>
                    </Container>
                    <If Operator="gt" Value="@HAL_TO_EAM_HATCH_FLOW_RATE" Value2="0.0">
                    	<True>
                    		<Polygon FillColor="#LineActive">
                        		<Vertex X="133" Y="400"/>
                        		<Vertex X="113" Y="390"/>
                        		<Vertex X="113" Y="410"/>
                    		</Polygon>
                    	</True>
                    	<False>
	                    	<Polygon FillColor="#LineActive">
                        		<Vertex X="27" Y="400"/>
                        		<Vertex X="47" Y="390"/>
                        		<Vertex X="47" Y="410"/>
                    		</Polygon>
                    	</False>
                    </If>
                </If>
                <Image X="72" Y="400" Width="120" Height="64" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/hatch_open.tga</Image>
            </True>
            <False>
                <Image X="72" Y="400" Width="120" Height="64" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/hatch_closed.tga</Image>
            </False>
        </If>
        <If Value="@HAL_IMV_3_SENSED_OPEN">
            <True>
                <Image X="332" Y="460" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/valve_on.tga</Image>
            </True>
            <False>
                <Image X="332" Y="460" Width="64" Height="64" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/valve_off.tga</Image>
            </False>
        </If>
        <If Value="@HAL_HATCH_OPEN_IND">
            <True>
                <If Operator="gt" Value1="@HAL_HATCH_FLOW_DISP" Value2="0.5">
                    <Line LineWidth="2" Color="#LineActive">
                        <Vertex X="230" Y="400"/>
                        <Vertex X="330" Y="400"/>
                    </Line>
                    <Container X="272" Y="350" Width="120" Height="32" HorizontalAlign="Center" VerticalAlign="Top">
                        <Rectangle FillColor="#BgColor" LineColor="#FgColor" LineWidth="1"/>
                        <String Size="#VerySmallFont" X="116" HorizontalAlign="Right" VerticalAlign="Middle" Color="#FgColor">@HAL_HATCH_FLOW_DISP(%.0f)lb/hr</String>
                    </Container>
                    <If Operator="lt" Value="@HAL_HATCH_FLOW_RATE" Value2="0.0">
                    	<True>
                    		<Polygon FillColor="#LineActive">
                        		<Vertex X="333" Y="400"/>
                        		<Vertex X="313" Y="390"/>
                        		<Vertex X="313" Y="410"/>
                    		</Polygon>
                    	</True>
                    	<False>
	                    	<Polygon FillColor="#LineActive">
                        		<Vertex X="227" Y="400"/>
                        		<Vertex X="247" Y="390"/>
                        		<Vertex X="247" Y="410"/>
                    		</Polygon>
                    	</False>
                    </If>
                </If>
                <Image X="289" Y="400" Width="120" Height="64" HorizontalAlign="Center" Rotate="180" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/hatch_open.tga</Image>
            </True>
            <False>
                <Image X="289" Y="400" Width="120" Height="64" HorizontalAlign="Center" Rotate="180" VerticalAlign="Middle">${dcappDisplayHome}/modules/subsystems/images/hatch_closed.tga</Image>
            </False>
        </If>

        <!-- ECLSS Box -->
        <Container X="550" Y="460" Width="240" Height="120" HorizontalAlign="Center" VerticalAlign="Middle">
            <Rectangle FillColor="0.6 0.6 0.6" LineColor="0.4 0.4 0.4" LineWidth="3"/>
            <String Size="#SmallFont" HorizontalAlign="Center" VerticalAlign="Middle" Color="0.2 0.2 0.2">#AirlockModuleName\nECLSS</String>
        </Container>

    </Container>

    <!-- Left Bezel Gutter -->
    <Container X="#lgutterx" Y="#l4y" Width="#GutterWidth" Height="#ButtonHeight2">
        <Line LineWidth="1" Color="#FgColor">
            <Vertex X="#lgutterlinex1" Y="#lgutterliney1"/>
            <Vertex X="#lgutterlinex2" Y="#lgutterliney2"/>
            <Vertex X="#lgutterlinex3" Y="#lgutterliney3"/>
            <Vertex X="#lgutterlinex4" Y="#lgutterliney4"/>
        </Line>
        <String Style="LeftGutter">#AirlockModuleName</String>
    </Container>
    <Container X="#lgutterx" Y="#l6y" Width="#GutterWidth" Height="#ButtonHeight2">
        <Line LineWidth="1" Color="#FgColor">
            <Vertex X="#lgutterlinex1" Y="#lgutterliney1"/>
            <Vertex X="#lgutterlinex2" Y="#lgutterliney2"/>
            <Vertex X="#lgutterlinex3" Y="#lgutterliney3"/>
            <Vertex X="#lgutterlinex4" Y="#lgutterliney4"/>
        </Line>
        <String Style="LeftGutter">#HabModuleName</String>
    </Container>
    <!-- Left Bezel -->
    <Button Style="LeftBezelButton" Type="Toggle" X="#lx" Y="#l3y" BezelKey="#l3" SwitchVariable="@HAL_HATCH_OPEN_CMD" IndicatorVariable="@HAL_HATCH_OPEN_IND">
        <On><Rectangle Style="LeftBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="LeftBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="LeftBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="LeftBezelButton">Hatch</String>
    </Button>
    <Button Style="LeftBezelButton" Type="Toggle" X="#lx" Y="#l4y" BezelKey="#l4" SwitchVariable="@HAL_IMV_3_POS_CMD" IndicatorVariable="@HAL_IMV_3_SENSED_OPEN">
        <On><Rectangle Style="LeftBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="LeftBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="LeftBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="LeftBezelButton">IMV</String>
    </Button>
    <Button Style="LeftBezelButton" Type="Toggle" X="#lx" Y="#l5y" BezelKey="#l5" SwitchVariable="@HAL_TO_EAM_HATCH_OPEN_CMD" IndicatorVariable="@HAL_TO_EAM_HATCH_OPEN_IND">
        <On><Rectangle Style="LeftBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="LeftBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="LeftBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="LeftBezelButton">Hatch</String>
    </Button>
    <Button Style="LeftBezelButton" Type="Toggle" X="#lx" Y="#l6y" BezelKey="#l6" SwitchVariable="@DECK_AFT_IMV_POS_CMD" IndicatorVariable="@DECK_AFT_IMV_POS_IND">
        <On><Rectangle Style="LeftBezelButton" FillColor="#ButtonOn"/></On>
        <Transition><Rectangle Style="LeftBezelButton" FillColor="#ButtonTrans"/></Transition>
        <Off><Rectangle Style="LeftBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="LeftBezelButton">IMV</String>
    </Button>

    <!-- Bottom Bezel Gutter -->

