<?xml version="1.0"?>
<Dummy>
    <Button Style="CornerBezelButton" X="#lx" Y="#l1y" BezelKey="#l1">
        <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff"/>
        <Image Width="46" Height="52" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/nexsys_common/images/home.tga</Image>
        <OnPress>
            <Set Variable="@HAL_SUBSYSTEM_DISPLAY">@ACTIVE_DISPLAY</Set>
            <Set Variable="@ACTIVE_DISPLAY">@SUBMENU1_DISPLAY</Set>
        </OnPress>
    </Button>

    <Button Style="TopBezelButton" X="#t1x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#HalSubsystemOverviewDisplay" BezelKey="#t1">
        <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="TopBezelButton">System\nOverview</String>
    </Button>

    <Button Style="TopBezelButton" X="#t2x" Y="#ty" BezelKey="#t2">
        <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
        <String Style="TopBezelButton">Power\nSystems</String>
        <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
        <OnPress>
            <Set Variable="@HAL_AMPS_SUBSYSTEM_DISPLAY">@ACTIVE_DISPLAY</Set>
            <Set Variable="@ACTIVE_DISPLAY">#HalAmspOverviewDisplay</Set>
        </OnPress>
    </Button>

    <Button Style="TopBezelButton" X="#t3x" Y="#ty" BezelKey="#t3">
        <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
        <String Style="TopBezelButton">Water\nSystems</String>
        <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
        <OnPress>
            <Set Variable="@HAL_SUBSYSTEM_DISPLAY">@ACTIVE_DISPLAY</Set>
            <Set Variable="@ACTIVE_DISPLAY">@SUBMENU9_DISPLAY</Set>
        </OnPress>
    </Button>

    <Button Style="TopBezelButton" X="#t4x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#HalTcsDisplay" BezelKey="#t4">
        <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="TopBezelButton">TCS</String>
    </Button>

    <Button Style="TopBezelButton" X="#t5x" Y="#ty" BezelKey="#t5">
        <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
        <String Style="TopBezelButton">Air\nSystems</String>
        <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
        <OnPress>
            <Set Variable="@HAL_SUBSYSTEM_DISPLAY">@ACTIVE_DISPLAY</Set>
            <Set Variable="@ACTIVE_DISPLAY">@SUBMENU11_DISPLAY</Set>
        </OnPress>
    </Button>

    <Button Style="CornerBezelButton" X="#rx" Y="#r1y" BezelKey="#r1">
        <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff"/>
        <Image Width="38" Height="52" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/nexsys_common/images/up.tga</Image>
        <OnPress>
            <Set Variable="@HAL_SUBSYSTEM_DISPLAY">@ACTIVE_DISPLAY</Set>
            <Set Variable="@ACTIVE_DISPLAY">@SUBMENU5_DISPLAY</Set>
        </OnPress>
    </Button>

    <Set Variable="@MENU_LABEL">Subsystems &gt; HAL Subsystem</Set>
</Dummy>
