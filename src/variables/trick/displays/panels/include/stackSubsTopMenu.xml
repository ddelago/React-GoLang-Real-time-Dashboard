<?xml version="1.0"?>
<Dummy>
    <Button Style="CornerBezelButton" X="#lx" Y="#l1y" BezelKey="#l1">
        <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff"/>
        <Image Width="46" Height="52" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/nexsys_common/images/home.tga</Image>
        <OnPress>
            <Set Variable="@SUBMENU5_DISPLAY">@ACTIVE_DISPLAY</Set>
            <Set Variable="@ACTIVE_DISPLAY">@SUBMENU1_DISPLAY</Set>
        </OnPress>
    </Button>

    <Button Style="TopBezelButton" X="#t1x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#StackDockingDisplay" BezelKey="#t1">
        <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="TopBezelButton">#{VisitingVechileName}-#{HabModuleName}\nAirflow</String>
    </Button>

    <Button Style="TopBezelButton" X="#t2x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#AirlockCabinDisplay" BezelKey="#t2">
        <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="TopBezelButton">#{HabModuleName}-#{AirlockModuleName}\nAirflow</String>
    </Button>

    <Button Style="TopBezelButton" X="#t3x" Y="#ty" BezelKey="#t3">
        <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
        <String Style="TopBezelButton">#VisitingVechileName\nSubsystem</String>
        <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
        <OnPress>
            <Set Variable="@SUBMENU5_DISPLAY">@ACTIVE_DISPLAY</Set>
            <Set Variable="@ACTIVE_DISPLAY">@MPCV_SUBSYSTEM_DISPLAY</Set>
        </OnPress>
    </Button>

    <Button Style="TopBezelButton" X="#t4x" Y="#ty" BezelKey="#t4">
        <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
        <String Style="TopBezelButton">#HabModuleName\nSubsystem</String>
        <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
        <OnPress>
            <Set Variable="@SUBMENU5_DISPLAY">@ACTIVE_DISPLAY</Set>
            <Set Variable="@ACTIVE_DISPLAY">@HAB_SUBSYSTEM_DISPLAY</Set>
        </OnPress>
    </Button>

    <If Operator="eq" Value1="#AirlockModuleName" Value2="HAL">
      <Button Style="TopBezelButton" X="#t5x" Y="#ty" BezelKey="#t5">
          <Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/>
          <String Style="TopBezelButton">#AirlockModuleName\nSubsystem</String>
          <Image Style="TopBezelButton">${dcappDisplayHome}/modules/nexsys_common/images/down-chevron.tga</Image>
          <OnPress>
              <Set Variable="@SUBMENU5_DISPLAY">@ACTIVE_DISPLAY</Set>
              <Set Variable="@ACTIVE_DISPLAY">@HAL_SUBSYSTEM_DISPLAY</Set>
          </OnPress>
      </Button>
    </If>

    <Button Style="TopBezelButton" X="#t6x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#CrewDisplay" BezelKey="#t6">
        <On><Rectangle Style="TopBezelButton" FillColor="0.3 0.45 0.75"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="0.4 0.4 0.4"/></Off>
        <String Style="TopBezelButton" Color="0.8 0.8 0.8" ShadowOffset="4">Crew</String>
    </Button>

    <If Value="#SubsystemOnly">
      <False>
        <Button Style="CornerBezelButton" X="#rx" Y="#r1y" BezelKey="#r1">
            <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff"/>
            <Image Width="38" Height="52" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/nexsys_common/images/up.tga</Image>
            <OnPress>
                <Set Variable="@SUBMENU5_DISPLAY">@ACTIVE_DISPLAY</Set>
                <Set Variable="@ACTIVE_DISPLAY">@SUBMENU1_DISPLAY</Set>
            </OnPress>
        </Button>
      </False>
    </If>

    <Set Variable="@MENU_LABEL">Subsystems</Set>
</Dummy>
