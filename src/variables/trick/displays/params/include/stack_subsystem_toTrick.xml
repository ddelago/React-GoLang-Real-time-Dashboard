<?xml version="1.0"?>
<Dummy>

  <!-- ********** Airlock Display Variable ********** -->
  <If Operator="eq" Value1="#AirlockModuleName" Value2="HAL">
    <True>
      <Constant Name="PARAM_SIM_PRE_NAME">HAL</Constant>
      <Include>hal_airlock_toTrick.xml</Include>
    </True>
    <False>
      <Constant Name="PARAM_SIM_PRE_NAME">AIRLOCK</Constant>
      <Include>airlock_toTrick.xml</Include>
    </False>
  </If>


  <!-- ********** MPCV Subsystem Variables ********** -->
  <Include>${dcappDisplayHome}/modules/mpcv/params/mpcv_ars_toTrick.xml</Include>

  <!-- MPCV PCS to trick parameter mapping -->
  <Constant Name="PARAM_SIM_PRE_NAME">ORION</Constant>
  <Constant Name="TRICK_SIM_PRE_NAME">mpcv_subsystems.fswIf.nominalCmd.eclss.pcs</Constant>
  <Include>${dcappDisplayHome}/modules/subsystems/params/pcs_toTrick.xml</Include>


  <!-- ********** HAB subsystem variables ********** -->
  <!-- HAB system alarm to trick parameter mapping -->
  <Constant Name="PARAM_SIM_PRE_NAME">HAB</Constant>
  <Constant Name="TRICK_SIM_PRE_NAME">hab_subsystems.fswIf.nominalCmd.system_alarm</Constant>
  <Include>${dcappDisplayHome}/modules/subsystems/params/subsystem_alarm_toTrick.xml</Include>

  <!-- HAB AMPS trick-dcapp mapping -->
  <Constant Name="PARAM_SIM_PRE_NAME">HAB_AMPS</Constant>
  <Constant Name="TRICK_SIM_PRE_NAME">hab_amps_fsw.amps_fsw_io.AmpsNominalCmd</Constant>
  <Include>${dcappDisplayHome}/modules/amps/params/amps_toTrick.xml</Include>

  <!-- Basic water display variable -->
  <Include>${dcappDisplayHome}/modules/hab/params/water_toTrick.xml</Include>

  <!-- Thermal control system variables -->
  <Include>${dcappDisplayHome}/modules/hab/params/itcs_toTrick.xml</Include>

  <Constant Name="PARAM_SIM_PRE_NAME">ETCSA</Constant>
  <Constant Name="TRICK_SIM_PRE_NAME">hab_subsystems.fswIf.nominalCmd.tcs.etcsA</Constant>
  <Include>${dcappDisplayHome}/modules/hab/params/etcs_toTrick.xml</Include>

  <Constant Name="PARAM_SIM_PRE_NAME">ETCSB</Constant>
  <Constant Name="TRICK_SIM_PRE_NAME">hab_subsystems.fswIf.nominalCmd.tcs.etcsB</Constant>
  <Include>${dcappDisplayHome}/modules/hab/params/etcs_toTrick.xml</Include>

  <!-- ARS Display Variables -->
  <Include>${dcappDisplayHome}/modules/hab/params/hab_cal_toTrick.xml</Include>
  <Include>${dcappDisplayHome}/modules/hab/params/eam_ars_toTrick.xml</Include>

  <!-- HAB PCS to trick parameter mapping -->
  <Constant Name="PARAM_SIM_PRE_NAME">EAM</Constant>
  <Constant Name="TRICK_SIM_PRE_NAME">hab_subsystems.fswIf.nominalCmd.eclss.pcs</Constant>
  <Include>${dcappDisplayHome}/modules/subsystems/params/pcs_toTrick.xml</Include>


  <!-- ********** HAL subsystem variables ********** -->
  <If Operator="eq" Value1="#AirlockModuleName" Value2="HAL">
    <!-- HAL system alarm to trick parameter mapping -->
    <Constant Name="PARAM_SIM_PRE_NAME">HAL</Constant>
    <Constant Name="TRICK_SIM_PRE_NAME">hal_subsystems.fswIf.nominalCmd.system_alarm</Constant>
    <Include>${dcappDisplayHome}/modules/subsystems/params/subsystem_alarm_toTrick.xml</Include>

    <!-- HAL AMPS trick-dcapp mapping -->
    <Constant Name="PARAM_SIM_PRE_NAME">HAL_AMPS</Constant>
    <Constant Name="TRICK_SIM_PRE_NAME">hal_amps_fsw.amps_fsw_io.AmpsNominalCmd</Constant>
    <Include>${dcappDisplayHome}/modules/amps/params/amps_toTrick.xml</Include>

    <Constant Name="PARAM_SIM_PRE_NAME">HAL</Constant>
    <Include>${dcappDisplayHome}/modules/hal/params/hal_potablewater_toTrick.xml</Include>
    <Include>${dcappDisplayHome}/modules/hal/params/hal_wastewater_toTrick.xml</Include>
    <Include>${dcappDisplayHome}/modules/hal/params/hal_tcs_toTrick.xml</Include>
    <Include>${dcappDisplayHome}/modules/hal/params/hal_ars_toTrick.xml</Include>
    <Include>${dcappDisplayHome}/modules/hal/params/hal_ars_rca_toTrick.xml</Include>

    <!-- HAL PCS to trick parameter mapping -->
    <Constant Name="PARAM_SIM_PRE_NAME">HAL</Constant>
    <Constant Name="TRICK_SIM_PRE_NAME">hal_subsystems.fswIf.nominalCmd.eclss.pcs</Constant>
    <Include>${dcappDisplayHome}/modules/subsystems/params/pcs_toTrick.xml</Include>
  </If>


  <!-- ********** Crew Display Variables ********** -->
  <Include>${dcappDisplayHome}/modules/subsystems/params/crew_toTrick.xml</Include>

</Dummy>
