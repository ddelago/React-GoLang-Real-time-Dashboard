<?xml version="1.0"?>
<Dummy>
    <Button Style="CornerBezelButton" X="#lx" Y="#l1y" BezelKey="#l1">
        <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff"/>
        <Image Width="46" Height="52" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/nexsys_common/images/home.tga</Image>
        <OnPress>
            <Set Variable="@HAB_AMPS_SUBSYSTEM_DISPLAY">@ACTIVE_DISPLAY</Set>
            <Set Variable="@ACTIVE_DISPLAY">@SUBMENU1_DISPLAY</Set>
        </OnPress>
    </Button>

    <Button Style="TopBezelButton" X="#t1x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#HabAmspOverviewDisplay" BezelKey="#t1">
        <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="TopBezelButton">AMPS \nOverview</String>
    </Button>

    <Button Style="TopBezelButton" X="#t2x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#HabAmpsPsDisplay" BezelKey="#t2">
        <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="TopBezelButton">Power \nSource</String>
    </Button>

    <Button Style="TopBezelButton" X="#t3x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#HabAmpsMbsuDisplay" BezelKey="#t3">
        <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="TopBezelButton">MBSU</String>
    </Button>

    <Button Style="TopBezelButton" X="#t4x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#HabAmpsPduDisplay" BezelKey="#t4">
        <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="TopBezelButton">PDU \n1 and 2</String>
    </Button>

    <Button Style="TopBezelButton" X="#t5x" Y="#ty" Variable="@ACTIVE_DISPLAY" On="#HabAmpsPdu2Display" BezelKey="#t5">
        <On><Rectangle Style="TopBezelButton" FillColor="#ButtonOn"/></On>
        <Off><Rectangle Style="TopBezelButton" FillColor="#ButtonOff"/></Off>
        <String Style="TopBezelButton">PDU \n3 and 4</String>
    </Button>

    <Button Style="CornerBezelButton" X="#rx" Y="#r1y" BezelKey="#r1">
        <Rectangle Style="CornerBezelButton" FillColor="#ButtonOff"/>
        <Image Width="38" Height="52" HorizontalAlign="Center" VerticalAlign="Middle">${dcappDisplayHome}/modules/nexsys_common/images/up.tga</Image>
        <OnPress>
            <Set Variable="@ACTIVE_DISPLAY">@#{PARAM_SIM_PRE_NAME}_SUBSYSTEM_DISPLAY</Set>
        </OnPress>
    </Button>

    <Set Variable="@MENU_LABEL">Subsystems &gt; HAB Subsystem &gt; Power Systems</Set>

    <Include>${dcappDisplayHome}/modules/nexsys_common/panels/standard_elements.xml</Include>
</Dummy>
